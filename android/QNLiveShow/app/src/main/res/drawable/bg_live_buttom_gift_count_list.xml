<?xml version="1.0" encoding="utf-8"?>
<shape  xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--corners - 边角弧度-->
    <corners
        android:radius="2.5dp"/>

    <!--solid - 填充色-->
    <solid
        android:color="@android:color/white"/>

    <!--stroke - 描边-->
    <stroke
        android:width="0.5dp"
        android:color="@android:color/white"/>
</shape>
