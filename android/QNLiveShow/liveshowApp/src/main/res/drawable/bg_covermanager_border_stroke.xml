<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--stroke属性
    width描边宽度
    color描边颜色
    dashWidth描边虚线的长度
    dashGap描边虚线之间的间隔-->
    <stroke
        android:dashGap="5dp"
        android:dashWidth="10dp"
        android:width="1dp"
        android:color="#7d7d7d" />
    <solid android:color="#FFFFFF" />
    <corners android:radius="3dp" />
</shape>