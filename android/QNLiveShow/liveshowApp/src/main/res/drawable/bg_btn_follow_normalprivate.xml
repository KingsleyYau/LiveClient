<?xml version="1.0" encoding="utf-8"?>
<shape  xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--corners - 边角弧度-->
    <corners
        android:radius="3dp"/>

    <!--solid - 填充色-->
    <solid
        android:color="#b0ff5abb"/>

    <!--stroke - 分割线-->
    <stroke
        android:width="1dp"
        android:color="@android:color/white"/>
</shape>
