<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="dotColor" format="color"/>
    <attr name="dotPadding" format="dimension"/>
    <attr name="dotOverPadding" format="dimension"/>
    <attr name="dotTextSize" format="dimension"/>
    <attr name="dotLocation" format="enum">
        <enum name="left" value="0"/>
        <enum name="rightTop" value="1"/>
        <enum name="rightBottom" value="2"/>
    </attr>

    <declare-styleable name="DotView">
        <attr name="dotColor"/>
        <attr name="dotPadding"/>
    </declare-styleable>

    <declare-styleable name="DotLayout">
        <attr name="dotColor"/>
        <attr name="dotPadding"/>
        <attr name="dotTextSize"/>
        <attr name="dotOverPadding"/>
        <attr name="dotLocation"/>
    </declare-styleable>

</resources>
